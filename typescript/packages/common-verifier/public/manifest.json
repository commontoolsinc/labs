{
  "manifest_version": 3,
  "name": "Common Verifier",
  "description": "Identify origins that meet Common standards for verifiable privacy",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon-unverified-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "./js/service-worker.js",
    "type": "module"
  }
}