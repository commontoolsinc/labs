{
  "name": "@commontools/common-builder",
  "author": "The Common Authors",
  "version": "0.0.1",
  "description": "Recipe and module builder framework.",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "npm run build && vitest",
    "build": "wireit",
    "clean": "wireit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commontoolsinc/labs.git"
  },
  "bugs": {
    "url": "https://github.com/commontoolsinc/labs/issues"
  },
  "homepage": "https://github.com/commontoolsinc/labs#readme",
  "exports": "./lib/index.js",
  "files": [
    "./lib/index.js"
  ],
  "dependencies": {
    "@commontools/common-runtime": "file:../common-runtime"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vitest": "^1.6.0",
    "wireit": "^0.14.4"
  },
  "wireit": {
    "build": {
      "dependencies": [],
      "files": [
        "./src/**/*"
      ],
      "output": [
        "./lib/**/*"
      ],
      "command": "tsc --build -f"
    },
    "clean": {
      "command": "rm -rf ./lib ./dist ./.wireit ./node_modules"
    }
  }
}
