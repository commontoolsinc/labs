{
  "name": "@commontools/tauri-shell",
  "version": "0.1.0",
  "tasks": {
    "check": "deno check src/mod.ts src/passkey-bridge.ts",
    "test": "deno test --allow-read test/",
    "tauri": "cargo tauri",
    "dev": "cargo tauri dev",
    "build": "cargo tauri build",
    "macos:dev": "cargo tauri dev",
    "macos:build": "cargo tauri build --target universal-apple-darwin",
    "macos:build-x64": "cargo tauri build --target x86_64-apple-darwin",
    "macos:build-arm": "cargo tauri build --target aarch64-apple-darwin",
    "android:init": "cargo tauri android init",
    "android:dev": "cargo tauri android dev",
    "android:build": "cargo tauri android build",
    "ios:init": "cargo tauri ios init",
    "ios:dev": "cargo tauri ios dev",
    "ios:build": "cargo tauri ios build"
  },
  "exports": {
    ".": "./src/mod.ts",
    "./passkey-bridge": "./src/passkey-bridge.ts"
  }
}
