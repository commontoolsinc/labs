{
  "name": "@commontools/runner",
  "tasks": {
    "test": "deno test --allow-ffi --allow-env --allow-read test/*.test.ts",
    "bench": {
      "description": "Run Cell benchmarks for performance analysis",
      "command": "deno bench --allow-read --allow-write --allow-net --allow-ffi --allow-env --no-check test/*.bench.ts"
    },
    "integration": "LOG_LEVEL=warn deno test -A ./integration/*.test.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./shared": "./src/shared.ts",
    "./schemas": "./src/schemas.ts",
    "./storage/inspector": "./src/storage/inspector.ts",
    "./storage/telemetry": "./src/storage/telemetry.ts",
    "./traverse": "./src/traverse.ts",
    "./storage/cache": "./src/storage/cache.ts",
    "./storage/cache.deno": "./src/storage/cache.deno.ts"
  }
}
